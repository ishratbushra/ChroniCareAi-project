n/Implementation/Data_files/Final_clean_Datafile.csv')\n","\n","# Step 2: Drop irrelevant and raw count columns\n","raw_count_cols = [\n","    'diabetes_mellitus(in thousand)', 'alzheimer_and_other_dementia (in thousand)',\n","    'asthma (in thousand)', 'cardiovascular_diseases (in thousand)',\n","    'kidney_diseases (in thousand)', 'population(in thousand)'\n","]\n","\n","irrelevant_cols = [\n","    'Palma Ratio', 'Inequality Group',\n","    'Prevalence of undernourishment (% of population)_imputed',\n","    'Government expenditure on education, total (% of GDP)_imputed',\n","    'Unemployment, total (% of total labor force) (modeled ILO estimate)_imputed',\n","    'Current health expenditure (% of GDP)_imputed',\n","    'Cause of death, by non-communicable diseases (% of total)_imputed',\n","    'Cause of death, by injury (% of total)_imputed',\n","    'Cause of death, by communicable diseases and maternal, prenatal and nutrition conditions (% of total)_imputed'\n","]\n","df.drop(columns=irrelevant_cols, inplace=True, errors='ignore')\n","\n","# Step 3: Select target and features for Diabetes\n","target = '%death by diabetes'\n","drop_cols = ['Year', 'Country Name', 'Country Code', target] + raw_count_cols\n","X = df.drop(columns=drop_cols, errors='ignore')\n","y = df[target].reset_index(drop=True)\n","\n","# Step 4: Feature scaling\n","scaler = StandardScaler()\n","X_scaled = scaler.fit_transform(X)\n","\n","# The original code uses a time series approach by country for diabetes.\n","# Let's reproduce that structure for the LSTM model training.\n","\n","# Step 5: Generate sequences by country (Reproducing the function from the diabetes section)\n","def create_sequences_by_country(df, features, target_col, time_steps=5):\n","    X_seq_list, y_seq_list = [], []\n","    df_sorted = df.sort_values(by=['Country Name', 'Year'])\n","    grouped = df_sorted.groupby('Country Name')\n","\n","    for _, group in grouped:\n","        # Ensure features DataFrame has the same index as the group\n","        X_group = features.loc[group.index].values\n","        y_group = df.loc[group.index, target_col].values\n","        # Only create sequences if the group has enough data points\n","        if len(X_group) >= time_steps:\n","            for i in range(len(X_group) - time_steps + 1): # Corrected indexing\n","                X_seq_list.append(X_group[i:i+time_steps])\n","                y_seq_list.append(y_group[i+time_steps-1]) # Predict the target at the end of the sequence\n","    return np.array(X_seq_list, dtype=np.float32), np.array(y_seq_list, dtype=np.float32)\n","\n","# Apply sequence creation using the scaled features DataFrame with the original df index\n","X_scaled_df = pd.DataFrame(X_scaled, columns=X.columns, index=df.index)\n","X_seq, y_seq = create_sequences_by_country(df, X_scaled_df, target, time_steps=5)\n","\n","\n","print(f\"\\nData shape after creating sequences: X_seq: {X_seq.shape}, y_seq: {y_seq.shape}\")\n","\n","# Step 6: 3-Fold Cross-Validation\n","kf = KFold(n_splits=3, shuffle=True, random_state=42)\n","mse_scores, r2_scores = [], []\n","\n","print(\"\\nStarting 3-Fold Cross-Validation for Diabetes with LSTM (Sequential Data)\")\n","for fold, (train_idx, val_idx) in enumerate(kf.split(X_seq)):\n","    print(f\"\\n Fold {fold + 1}\")\n","    tf.keras.backend.clear_session() # Clear session for fresh start in each fold\n","\n","    X_train_cv, X_val_cv = X_seq[train_idx], X_seq[val_idx]\n","    y_train_cv, y_val_cv = y_seq[train_idx], y_seq[val_idx]\n","\n","    # Define the LSTM model (using the structure from the original code)\n","    model = Sequential([\n","        LSTM(64,\n","             input_shape=(X_train_cv.shape[1], X_train_cv.shape[2]),\n","             activation='tanh',\n","             recurrent_activation='sigmoid',\n","             # Keeping original dropout/recurrent_dropout as 0.0 based on the provided code snippet\n","             dropout=0.0,\n","             recurrent_dropout=0.0,\n","             return_sequences=False,\n","             unroll=False,\n","             use_bias=True),\n","        Dropout(0.2), # Added dropout after LSTM as seen in the original code\n","        Dense(32, activation='relu'),\n","        Dense(1) # Output layer for regression\n","    ])\n","    model.compile(optimizer='adam', loss='mse')\n","\n","    # Early stopping callback\n","    early_stop = EarlyStopping(monitor='val_loss', patience=10, restore_best_weights=True)\n","\n","    # Train the model\n","    history = model.fit(X_train_cv, y_train_cv,\n","                        validation_data=(X_val_cv, y_val_cv),\n","                        epochs=100,\n","                        batch_size=16,\n","                        verbose=0, # Set verbose to 0 for cleaner output during CV\n","                        callbacks=[early_stop])\n","\n","    # Evaluate the model on the validation fold\n","    y_pred_cv = model.predict(X_val_cv).flatten()\n","    mse_scores.append(mean_squared_error(y_val_cv, y_pred_cv))\n","    r2_scores.append(r2_score(y_val_cv, y_pred_cv))\n","\n","    print(f\" Fold {fold + 1} finished. Validation MSE: {mse_scores[-1]:.4f}, R¬≤: {r2_scores[-1]:.4f}\")\n","\n","\n","print(\"\\n Cross-Validation Results:\")\n","print(\"Average MSE:\", round(np.mean(mse_scores), 4))\n","print(\"Average R¬≤:\", round(np.mean(r2_scores), 4))\n","\n","\n","# Step 7: Hyperparameter Tuning using Keras Tuner\n","print(\"\\nStarting Hyperparameter Tuning for Diabetes with LSTM\")\n","\n","# Split the sequence data for tuning\n","X_train, X_test, y_train, y_test = train_test_split(X_seq, y_seq, test_size=0.2, random_state=42)\n","\n","def build_lstm_model_tuned(hp):\n","    model = Sequential()\n","    model.add(LSTM(units=hp.Int('units', 32, 128, step=32),\n","                   input_shape=(X_train.shape[1], X_train.shape[2]),\n","                   activation='tanh', # Keeping consistent with the previous model\n","                   recurrent_activation='sigmoid', # Keeping consistent\n","                   # Keeping original dropout/recurrent_dropout as 0.0 for the LSTM layer\n","                   dropout=0.0,\n","                   recurrent_dropout=0.0,\n","                   return_sequences=False,\n","                   unroll=False,\n","                   use_bias=True))\n","    model.add(Dropout(hp.Float('dropout', 0.2, 0.5, step=0.1))) # Dropout after LSTM is tunable\n","    model.add(Dense(32, activation='relu')) # Fixed Dense layer\n","    model.add(Dense(1)) # Output layer\n","\n","    model.compile(optimizer=hp.Choice('optimizer', ['adam', 'rmsprop']),\n","                  loss='mse')\n","    return model\n","\n","# Configure and run the tuner\n","tuner = kt.RandomSearch(\n","    build_lstm_model_tuned,\n","    objective='val_loss',\n","    max_trials=10, # Number of hyperparameter combinations to try\n","    executions_per_trial=1, # Number of times to train the model with the same hyperparameters\n","    directory='diabetes_lstm_tuning', # Directory to save results\n","    project_name='diabetes_lstm_final_tuned' # Project name\n",")\n","\n","# Early stopping for tuning\n","early_stop_tune = EarlyStopping(monitor='val_loss', patience=10, restore_best_weights=True)\n","\n","# Start the search\n","tuner.search(X_train, y_train,\n","             validation_split=0.2, # Use a validation split from the training data for tuning\n","             epochs=100,\n","             batch_size=32, # Batch size for tuning\n","             callbacks=[early_stop_tune],\n","             verbose=1) # Set verbose to 1 to see tuning progress\n","\n","# Print tuning summary\n","tuner.results_summary()\n","\n","# Step 8: Final Evaluation on the Test Set\n","print(\"\\nStarting Final Evaluation on Test Set with Best Model\")\n","# Get the best model\n","best_model = tuner.get_best_models(num_models=1)[0]\n","\n","# Evaluate the best model on the held-out test set\n","y_pred_test = best_model.predict(X_test).flatten()\n","\n","# Calculate final metrics\n","mse_final = mean_squared_error(y_test, y_pred_test)\n","r2_final = r2_score(y_test, y_pred_test)\n","\n","print(\"\\n Final Evaluation on Test Set:\")\n","print(\"MSE:\", round(mse_final, 4))\n","print(\"R¬≤ Score:\", round(r2_final, 4))\n","\n","# Print best hyperparameters found by the tuner\n","best_hps = tuner.get_best_hyperparameters(1)[0]\n","print(\"\\nüîß Best Hyperparameters found by Keras Tuner:\")\n","print(\"LSTM Units:\", best_hps.get('units'))\n","print(\"Dropout (after LSTM):\", best_hps.get('dropout'))\n","print(\"Optimizer:\", best_hps.get('optimizer'))\n","\n","# Optional: Save the best model\n","# best_model.save('best_diabetes_lstm_model.h5')\n","# print(\"\\nBest model saved to best_diabetes_lstm_model.h5\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"RGnlcjSGkruS","executionInfo":{"status":"ok","timestamp":1751047842139,"user_tz":240,"elapsed":119002,"user":{"displayName":"Muhammad Zaka","userId":"07422164627874822290"}},"outputId":"4939ce8d-d47f-4af0-8868-25e6022ab7f4"},"execution_count":null,"outputs":[{"output_type":"stream","name":"stdout","text":["Trial 10 Complete [00h 00m 07s]\n","val_loss: 1.447668194770813\n","\n","Best val_loss So Far: 1.357204556465149\n","Total elapsed time: 00h 01m 24s\n","Results summary\n","Results in diabetes_lstm_tuning/diabetes_lstm_final_tuned\n","Showing 10 best trials\n","Objective(name=\"val_loss\", direction=\"min\")\n","\n","Trial 06 summary\n","Hyperparameters:\n","units: 96\n","dropout: 0.30000000000000004\n","optimizer: rmsprop\n","Score: 1.357204556465149\n","\n","Trial 09 summary\n","Hyperparameters:\n","units: 128\n","dropout: 0.4\n","optimizer: rmsprop\n","Score: 1.447668194770813\n","\n","Trial 04 summary\n","Hyperparameters:\n","units: 128\n","dropout: 0.4\n","optimizer: adam\n","Score: 1.6389658451080322\n","\n","Trial 08 summary\n","Hyperparameters:\n","units: 128\n","dropout: 0.2\n","optimizer: rmsprop\n","Score: 1.692047357559204\n","\n","Trial 01 summary\n","Hyperparameters:\n","units: 128\n","dropout: 0.2\n","optimizer: adam\n","Score: 1.7528527975082397\n","\n","Trial 05 summary\n","Hyperparameters:\n","units: 128\n","dropout: 0.30000000000000004\n","optimizer: adam\n","Score: 1.8660651445388794\n","\n","Trial 03 summary\n","Hyperparameters:\n","units: 64\n","dropout: 0.30000000000000004\n","optimizer: adam\n","Score: 2.1531901359558105\n","\n","Trial 02 summary\n","Hyperparameters:\n","units: 64\n","dropout: 0.4\n","optimizer: rmsprop\n","Score: 2.325770854949951\n","\n","Trial 07 summary\n","Hyperparameters:\n","units: 32\n","dropout: 0.4\n","optimizer: rmsprop\n","Score: 3.36958646774292\n","\n","Trial 00 summary\n","Hyperparameters:\n","units: 32\n","dropout: 0.4\n","optimizer: adam\n","Score: 3.830014705657959\n","\n","Starting Final Evaluation on Test Set with Best Model\n","\u001b[1m1/3\u001b[0m \u001b[32m‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ\u001b[0m\u001b[37m‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ\u001b[0m \u001b[1m0s\u001b[0m 108ms/step"]},{"output_type":"stream","name":"stderr","text":["/usr/local/lib/python3.11/dist-packages/keras/src/saving/saving_lib.py:757: UserWarning: Skipping variable loading for optimizer 'rmsprop', because it has 2 variables whereas the saved optimizer has 9 variables. \n","  saveable.load_own_variables(weights_store.get(inner_path))\n"]},{"output_type":"stream","name":"stdout","text":["\u001b[1m3/3\u001b[0m \u001b[32m‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ\u001b[0m\u001b[37m\u001b[0m \u001b[1m0s\u001b[0m 55ms/step\n","\n"," Final Evaluation on Test Set:\n","MSE: 3.9258\n","R¬≤ Score: 0.5327\n","\n","üîß Best Hyperparameters found by Keras Tuner:\n","LSTM Units: 96\n","Dropout (after LSTM): 0.30000000000000004\n","Optimizer: rmsprop\n"]}]},{"cell_type":"code","source":["# GRU\n","# Required Libraries\n","import pandas as pd\n","import numpy as np\n","from sklearn.model_selection import KFold, train_test_split\n","from sklearn.preprocessing import StandardScaler\n","from sklearn.metrics import mean_squared_error, r2_score\n","\n","from tensorflow.keras.models import Sequential\n","from tensorflow.keras.layers import GRU, Dropout, Dense\n","from tensorflow.keras.callbacks import EarlyStopping\n","\n","import keras_tuner as kt\n","\n","# Step 1: Load and Clean the Data\n","df = pd.read_csv('/content/drive/MyDrive/ChronicCare AI Data Competition/Implementation/Data_files/Final_clean_Datafile.csv')\n","\n","raw_count_cols = [\n","    'diabetes_mellitus(in thousand)', 'alzheimer_and_other_dementia (in thousand)',\n","    'asthma (in thousand)', 'cardiovascular_diseases (in thousand)',\n","    'kidney_diseases (in thousand)', 'population(in thousand)'\n","]\n","\n","columns_to_drop = [\n","    'Palma Ratio',\n","    'Inequality Group',\n","    'Prevalence of undernourishment (% of population)_imputed',\n","    'Government expenditure on education, total (% of GDP)_imputed',\n","    'Unemployment, total (% of total labor force) (modeled ILO estimate)_imputed',\n","    'Current health expenditure (% of GDP)_imputed',\n","    'Cause of death, by non-communicable diseases (% of total)_imputed',\n","    'Cause of death, by injury (% of total)_imputed',\n","    'Cause of death, by communicable diseases and maternal, prenatal and nutrition conditions (% of total)_imputed'\n","]\n","df.drop(columns=columns_to_drop, errors='ignore', inplace=True)\n","\n","target = '%death by diabetes'\n","drop_cols = ['Year', 'Country Name', 'Country Code', target] + raw_count_cols\n","X = df.drop(columns=drop_cols, errors='ignore')\n","y = df[target].reset_index(drop=True)\n","\n","# Step 2: Scale and Reshape Data\n","scaler = StandardScaler()\n","X_scaled = scaler.fit_transform(X)\n","X_scaled = X_scaled.reshape((X_scaled.shape[0], 1, X_scaled.shape[1]))\n","\n","# Step 3: 3-Fold Cross-Validation for Fixed GRU\n","kf = KFold(n_splits=3, shuffle=True, random_state=42)\n","r2_scores_gru = []\n","mse_scores_gru = []\n","\n","for fold, (train_idx, val_idx) in enumerate(kf.split(X_scaled)):\n","    print(f\"\\n GRU Fold {fold + 1}\")\n","\n","    X_train_cv, X_val_cv = X_scaled[train_idx], X_scaled[val_idx]\n","    y_train_cv, y_val_cv = y[train_idx], y[val_idx]\n","\n","    model_gru = Sequential()\n","    model_gru.add(GRU(64, input_shape=(X_train_cv.shape[1], X_train_cv.shape[2]), return_sequences=False))\n","    model_gru.add(Dropout(0.2))\n","    model_gru.add(Dense(32, activation='relu'))\n","    model_gru.add(Dense(1))\n","    model_gru.compile(optimizer='adam', loss='mse')\n","\n","    early_stop_cv = EarlyStopping(monitor='val_loss', patience=10, restore_best_weights=True)\n","    model_gru.fit(X_train_cv, y_train_cv,\n","                  validation_data=(X_val_cv, y_val_cv),\n","                  epochs=100, batch_size=16, verbose=0,\n","                  callbacks=[early_stop_cv])\n","\n","    y_pred_cv = model_gru.predict(X_val_cv).flatten()\n","    mse_fold = mean_squared_error(y_val_cv, y_pred_cv)\n","    r2_fold = r2_score(y_val_cv, y_pred_cv)\n","\n","    mse_scores_gru.append(mse_fold)\n","    r2_scores_gru.append(r2_fold)\n","\n","    print(f\"Fold {fold + 1} ‚Äî MSE: {round(mse_fold, 4)}, R¬≤: {round(r2_fold, 4)}\")\n","\n","print(\"\\n Final GRU Cross-Validation Results:\")\n","print(\"Average MSE:\", round(np.mean(mse_scores_gru), 4))\n","print(\"Average R¬≤ Score:\", round(np.mean(r2_scores_gru), 4))\n","\n","# Step 4: Hyperparameter Tuning with Keras Tuner\n","\n","# Redefine Train/Test split for tuning\n","X_train, X_test, y_train, y_test = train_test_split(X_scaled, y, test_size=0.2, random_state=42)\n","\n","def build_gru_model(hp):\n","    model = Sequential()\n","    model.add(GRU(\n","        units=hp.Int('units', min_value=32, max_value=128, step=32),\n","        input_shape=(X_train.shape[1], X_train.shape[2]),\n","        return_sequences=False\n","    ))\n","    model.add(Dropout(hp.Float('dropout', 0.2, 0.5, step=0.1)))\n","    model.add(Dense(32, activation='relu'))\n","    model.add(Dense(1))\n","\n","    model.compile(\n","        optimizer=hp.Choice('optimizer', ['adam', 'rmsprop']),\n","        loss='mse'\n","    )\n","    return model\n","\n","tuner_gru = kt.RandomSearch(\n","    build_gru_model,\n","    objective='val_loss',\n","    max_trials=10,\n","    executions_per_trial=1,\n","    directory='gru_tuning',\n","    project_name='diabetes_gru'\n",")\n","\n","early_stop_tune = EarlyStopping(monitor='val_loss', patience=10, restore_best_weights=True)\n","\n","tuner_gru.search(X_train, y_train,\n","                 validation_split=0.2,\n","                 epochs=100,\n","                 batch_size=32,\n","                 callbacks=[early_stop_tune],\n","                 verbose=1)\n","\n","# Step 5: Evaluate Best GRU Model\n","best_gru_model = tuner_gru.get_best_models(num_models=1)[0]\n","y_pred_gru = best_gru_model.predict(X_test).flatten()\n","\n","mse_gru = mean_squared_error(y_test, y_pred_gru)\n","r2_gru = r2_score(y_test, y_pred_gru)\n","\n","print(\"\\n Best GRU Model (Post-Tuning) Performance:\")\n","print(\"Test MSE:\", round(mse_gru, 4))\n","print(\"Test R¬≤ Score:\", round(r2_gru, 4))\n","\n","best_hps = tuner_gru.get_best_hyperparameters(1)[0]\n","print(\"\\n Best Hyperparameters:\")\n","print(f\"Units: {best_hps.get('units')}\")\n","print(f\"Dropout: {best_hps.get('dropout')}\")\n","print(f\"Optimizer: {best_hps.get('optimizer')}\")"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"DuA9dHsT_zZ3","executionInfo":{"status":"ok","timestamp":1751044115825,"user_tz":240,"elapsed":270475,"user":{"displayName":"Muhammad Zaka","userId":"07422164627874822290"}},"outputId":"1021853b-5d5e-4a14-fa7b-25e0332a2ca9","collapsed":true},"execution_count":8,"outputs":[{"output_type":"stream","name":"stdout","text":["Trial 10 Complete [00h 00m 17s]\n","val_loss: 1.54844069480896\n","\n","Best val_loss So Far: 1.4346789121627808\n","Total elapsed time: 00h 03m 13s\n","\u001b[1m1/7\u001b[0m \u001b[32m‚îÅ‚îÅ\u001b[0m\u001b[37m‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ\u001b[0m \u001b[1m0s\u001b[0m 86ms/step"]},{"output_type":"stream","name":"stderr","text":["/usr/local/lib/python3.11/dist-packages/keras/src/layers/rnn/rnn.py:200: UserWarning: Do not pass an `input_shape`/`input_dim` argument to a layer. When using Sequential models, prefer using an `Input(shape)` object as the first layer in the model instead.\n","  super().__init__(**kwargs)\n","/usr/local/lib/python3.11/dist-packages/keras/src/saving/saving_lib.py:757: UserWarning: Skipping variable loading for optimizer 'adam', because it has 2 variables whereas the saved optimizer has 16 variables. \n","  saveable.load_own_variables(weights_store.get(inner_path))\n"]},{"output_type":"stream","name":"stdout","text":["\u001b[1m7/7\u001b[0m \u001b[32m‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ\u001b[0m\u001b[37m\u001b[0m \u001b[1m0s\u001b[0m 16ms/step\n","\n"," Best GRU Model (Post-Tuning) Performance:\n","Test MSE: 1.4542\n","Test R¬≤ Score: 0.802\n","\n"," Best Hyperparameters:\n","Units: 64\n","Dropout: 0.2\n","Optimizer: adam\n"]}]},{"cell_type":"markdown","source":["# Alzheimer"],"metadata":{"id":"to62p5fHEI__"}},{"cell_type":"code","source":["#LSTM"],"metadata":{"id":"VDAFa28eFejX"},"execution_count":null,"outputs":[]},{"cell_type":"code","source":["# Install Keras Tuner\n","!pip install keras-tuner --quiet\n","\n","# Imports\n","import pandas as pd\n","import numpy as np\n","from sklearn.model_selection import KFold, train_test_split\n","from sklearn.preprocessing import StandardScaler\n","from sklearn.metrics import mean_squared_error, r2_score\n","\n","from tensorflow.keras.models import Sequential\n","from tensorflow.keras.layers import LSTM, Dropout, Dense\n","from tensorflow.keras.callbacks import EarlyStopping\n","\n","import keras_tuner as kt\n","\n","# Load Data\n","df = pd.read_csv('/content/drive/MyDrive/ChronicCare AI Data Competition/Implementation/Data_files/Final_clean_Datafile.csv')\n","\n","raw_count_cols = [\n","    'diabetes_mellitus(in thousand)', 'alzheimer_and_other_dementia (in thousand)',\n","    'asthma (in thousand)', 'cardiovascular_diseases (in thousand)',\n","    'kidney_diseases (in thousand)', 'population(in thousand)'\n","]\n","# Drop irrelevant/imputed columns\n","columns_to_drop = [\n","    'Palma Ratio',\n","    'Inequality Group',\n","    'Prevalence of undernourishment (% of population)_imputed',\n","    'Government expenditure on education, total (% of GDP)_imputed',\n","    'Unemployment, total (% of total labor force) (modeled ILO estimate)_imputed',\n","    'Current health expenditure (% of GDP)_imputed',\n","    'Cause of death, by non-communicable diseases (% of total)_imputed',\n","    'Cause of death, by injury (% of total)_imputed',\n","    'Cause of death, by communicable diseases and maternal, prenatal and nutrition conditions (% of total)_imputed'\n","]\n","df = df.drop(columns=columns_to_drop, errors='ignore')\n","\n","# Target\n","target = '%death by alzheimer_and_other_dementia'\n","drop_cols = ['Year', 'Country Name', 'Country Code', target] + raw_count_cols\n","X = df.drop(columns=drop_cols, errors='ignore')\n","y = df[target].reset_index(drop=True)\n","\n","# Preprocessing\n","scaler = StandardScaler()\n","X_scaled = scaler.fit_transform(X)\n","X_scaled = X_scaled.reshape((X_scaled.shape[0], 1, X_scaled.shape[1]))\n","\n","# 3-Fold Cross-Validation\n","kf = KFold(n_splits=3, shuffle=True, random_state=42)\n","mse_scores, r2_scores = [], []\n","\n","for fold, (train_idx, val_idx) in enumerate(kf.split(X_scaled)):\n","    print(f\"\\n Fold {fold + 1}\")\n","    X_train_cv, X_val_cv = X_scaled[train_idx], X_scaled[val_idx]\n","    y_train_cv, y_val_cv = y[train_idx], y[val_idx]\n","\n","    model = Sequential([\n","        LSTM(64, input_shape=(X_train_cv.shape[1], X_train_cv.shape[2])),\n","        Dropout(0.2),\n","        Dense(32, activation='relu'),\n","        Dense(1)\n","    ])\n","    model.compile(optimizer='adam', loss='mse')\n","\n","    early_stop = EarlyStopping(monitor='val_loss', patience=10, restore_best_weights=True)\n","    model.fit(X_train_cv, y_train_cv, validation_data=(X_val_cv, y_val_cv),\n","              epochs=100, batch_size=16, verbose=0, callbacks=[early_stop])\n","\n","    y_pred_cv = model.predict(X_val_cv).flatten()\n","    mse = mean_squared_error(y_val_cv, y_pred_cv)\n","    r2 = r2_score(y_val_cv, y_pred_cv)\n","\n","    mse_scores.append(mse)\n","    r2_scores.append(r2)\n","\n","print(\"\\n Cross-Validation Results:\")\n","print(\"Average MSE:\", round(np.mean(mse_scores), 4))\n","print(\"Average R¬≤:\", round(np.mean(r2_scores), 4))\n","\n","# Hyperparameter Tuning with Keras Tuner\n","X_train, X_test, y_train, y_test = train_test_split(X_scaled, y, test_size=0.2, random_state=42)\n","\n","def build_model(hp):\n","    model = Sequential()\n","    model.add(LSTM(\n","        units=hp.Int('units', 32, 128, step=32),\n","        input_shape=(X_train.shape[1], X_train.shape[2])\n","    ))\n","    model.add(Dropout(hp.Float('dropout', 0.2, 0.5, step=0.1)))\n","    model.add(Dense(32, activation='relu'))\n","    model.add(Dense(1))\n","    model.compile(optimizer=hp.Choice('optimizer', ['adam', 'rmsprop']),\n","                  loss='mse')\n","    return model\n","\n","tuner = kt.RandomSearch(\n","    build_model,\n","    objective='val_loss',\n","    max_trials=10,\n","    executions_per_trial=1,\n","    directory='lstm_alzheimer_tuning',\n","    project_name='alzheimer_lstm'\n",")\n","\n","tuner.search(X_train, y_train,\n","             validation_split=0.2,\n","             epochs=100,\n","             batch_size=32,\n","             callbacks=[EarlyStopping(monitor='val_loss', patience=10, restore_best_weights=True)],\n","             verbose=1)\n","\n","# Final Evaluation\n","best_model = tuner.get_best_models(1)[0]\n","y_pred_test = best_model.predict(X_test).flatten()\n","mse_final = mean_squared_error(y_test, y_pred_test)\n","r2_final = r2_score(y_test, y_pred_test)\n","\n","print(\"\\n Final Best Model Results:\")\n","print(\"Test MSE:\", round(mse_final, 4))\n","print(\"Test R¬≤:\", round(r2_final, 4))\n","\n","best_hps = tuner.get_best_hyperparameters(1)[0]\n","print(\"\\n Best Hyperparameters:\")\n","print(\"Units:\", best_hps.get('units'))\n","print(\"Dropout:\", best_hps.get('dropout'))\n","print(\"Optimizer:\", best_hps.get('optimizer'))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"UGTjNC6PEY5x","executionInfo":{"status":"ok","timestamp":1751044564205,"user_tz":240,"elapsed":271131,"user":{"displayName":"Muhammad Zaka","userId":"07422164627874822290"}},"outputId":"ee1e4a59-1d28-4708-c80b-d136fc4165af"},"execution_count":9,"outputs":[{"output_type":"stream","name":"stdout","text":["Trial 10 Complete [00h 00m 20s]\n","val_loss: 0.6042773127555847\n","\n","Best val_loss So Far: 0.48454391956329346\n","Total elapsed time: 00h 03m 13s\n"]},{"output_type":"stream","name":"stderr","text":["/usr/local/lib/python3.11/dist-packages/keras/src/layers/rnn/rnn.py:200: UserWarning: Do not pass an `input_shape`/`input_dim` argument to a layer. When using Sequential models, prefer using an `Input(shape)` object as the first layer in the model instead.\n","  super().__init__(**kwargs)\n","/usr/local/lib/python3.11/dist-packages/keras/src/saving/saving_lib.py:757: UserWarning: Skipping variable loading for optimizer 'adam', because it has 2 variables whereas the saved optimizer has 16 variables. \n","  saveable.load_own_variables(weights_store.get(inner_path))\n"]},{"output_type":"stream","name":"stdout","text":["\u001b[1m7/7\u001b[0m \u001b[32m‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ‚îÅ\u001b[0m\u001b[37m\u001b[0m \u001b[1m0s\u001b[0m 19ms/step\n","\n"," Final Best Model Results:\n","Test MSE: 0.8937\n","Test R¬≤: 0.8605\n","\n"," Best Hyperparameters:\n","Units: 96\n","Dropout: 0.2\n","Optimizer: adam\n"]}]},{"cell_type":"code","source":["#GRU\n","# Install Keras Tuner if not yet installed\n","!pip install keras-tuner --quiet\n","\n","# Imports\n","import pandas as pd\n","import numpy as np\n","from sklearn.model_selection import KFold, train_test_split\n","from sklearn.preprocessing import StandardScaler\n","from sklearn.metrics import mean_squared_error, r2_score\n","\n","from tensorflow.keras.models import Sequential\n","from tensorflow.keras.layers import GRU, Dropout, Dense\n","from tensorflow.keras.callbacks import EarlyStopping\n","import keras_tuner as kt\n","\n","# Load data\n","df = pd.read_csv('/content/drive/MyDrive/ChronicCare AI Data Competition/Implementation/Data_files/Final_clean_Datafile.csv')\n","\n","raw_count_cols = [\n","    'diabetes_mellitus(in thousand)', 'alzheimer_and_other_dementia (in thousand)',\n","    'asthma (in thousand)', 'cardiovascular_diseases (in thousand)',\n","    'kidney_diseases (in thousand)', 'population(in thousand)'\n","]\n","# Drop unnecessary/imputed columns\n","columns_to_drop = [\n","    'Palma Ratio',\n","    'Inequality Group',\n","    'Prevalence of undernourishment (% of population)_imputed',\n","    'Government expenditure on education, total (% of GDP)_imputed',\n","    'Unemployment, total (% of total labor force) (modeled ILO estimate)_imputed',\n","    'Current health expenditure (% of GDP)_imputed',\n","    'Cause of death, by non-communicable diseases (% of total)_imputed',\n","    'Cause of death, by injury (% of total)_imputed',\n","    'Cause of death, by communicable diseases and maternal, prenatal and nutrition conditions (% of total)_imputed'\n","]\n","df = df.drop(columns=columns_to_drop, errors='ignore')\n","\n","# Target and feature separation\n","target = '%death by alzheimer_and_other_dementia'\n","drop_cols = ['Year', 'Country Name', 'Country Code', target] + raw_count_cols\n","X = df.drop(columns=drop_cols, errors='ignore')\n","y = df[target].reset_index(drop=True)\n","\n","# Scaling and reshaping for GRU\n","scaler = StandardScaler()\n","X_scaled = scaler.fit_transform(X)\n","X_scaled = X_scaled.reshape((X_scaled.shape[0], 1, X_scaled.shape[1]))\n","\n","# 3-Fold Cross-Validation with fixed GRU\n","kf = KFold(n_splits=3, shuffle=True, random_state=42)\n","mse_scores, r2_scores = [], []\n","\n","for fold, (train_idx, val_idx) in enumerate(kf.split(X_scaled)):\n","    print(f\"\\n GRU Fold {fold + 1}\")\n","    X_train_cv, X_val_cv = X_scaled[train_idx], X_scaled[val_idx]\n","    y_train_cv, y_val_cv = y[train_idx], y[val_idx]\n","\n","    model = Sequential([\n","        GRU(64, input_shape=(X_train_cv.shape[1], X_train_cv.shape[2])),\n","        Dropout(0.2),\n","        Dense(32, activation='relu'),\n","        Dense(1)\n","    ])\n","    model.compile(optimizer='adam', loss='mse')\n","\n","    early_stop = EarlyStopping(monitor='val_loss', patience=10, restore_best_weights=True)\n","    model.fit(X_train_cv, y_train_cv, validation_data=(X_val_cv, y_val_cv),\n","              epochs=100, batch_size=16, verbose=0, callbacks=[early_stop])\n","\n","    y_pred_cv = model.predict(X_val_cv).flatten()\n","    mse = mean_squared_error(y_val_cv, y_pred_cv)\n","    r2 = r2_score(y_val_cv, y_pred_cv)\n","\n","    mse_scores.append(mse)\n","    r2_scores.append(r2)\n","\n","print(\"\\n GRU Cross-Validation Results:\")\n","print(\"Average MSE:\", round(np.mean(mse_scores), 4))\n","print(\"Average R¬≤:\", round(np.mean(r2_scores), 4))\n","\n","# Hyperparameter Tuning for GRU with Keras Tuner\n","X_train, X_test, y_train, y_test = train_test_split(X_scaled, y, test_size=0.2, random_state=42)\n","\n","def build_gru_model(hp):\n","    model = Sequential()\n","    model.add(GRU(\n","        units=hp.Int('units', 32, 128, step=32),\n","        input_shape=(X_train.shape[1], X_train.shape[2])\n","    ))\n","    model.add(Dropout(hp.Float('dropout', 0.2, 0.5, step=0.1)))\n","    model.add(Dense(32, activation='relu'))\n","    model.add(Dense(1))\n","    model.compile(\n","        optimizer=hp.Choice('optimizer', ['adam', 'rmsprop']),\n","        loss='mse'\n","    )\n","    return model\n","\n","tuner = kt.RandomSearch(\n","    build_gru_model,\n","    objective='val_loss',\n","    max_trials=10,\n","    executions_per_trial=1,\n","    directory='gru_tuning_alzheimers',\n","    project_name='alzheimers_gru'\n",")\n","\n","early_stop_tune = EarlyStopping(monitor='val_loss', patience=10, restore_best_weights=True)\n","\n","tuner.search(X_train, y_train,\n","             validation_split=0.2,\n","             epochs=100,\n","             batch_size=32,\n","             callbacks=[early_stop_tune],\n","             verbose=1)\n","\n","# Final Test Set Evaluation\n","best_model = tuner.get_best_models(num_models=1)[0]\n","y_pred_test = best_model.predict(X_test).flatten()\n","\n","mse_final = mean_squared_error(y_test, y_pred_test)\n","r2_final = r2_score(y_test, y_pred_test)\n","\n","print(\"\\n Best GRU Model Results on Test Set:\")\n","print(\"Test MSE:\", round(mse_final, 4))\n","print(\"Test R¬≤:\", round(r2_final, 4))\n","\n","best_hps = tuner.get_best_hyperparameters(1)[0]\n","print(\"\\n Best Hyperparameters for GRU:\")\n","print(\"Units:\", best_hps.get('units'))\n","print(\"Dropout:\", best_hps.get('dropout'))\n","print(\"Optimizer:\", best_hps.get('optimizer'))"],"metadata":{"colab":{"base_uri":"https://localhost:8080/"},"id":"Oww-fic8FkH1","executionInfo":{"status":"ok","timestamp":1751044899560,"user_tz":240,"elapsed":265218,"user":{"displayName":"Muhammad Zaka","userId":"07422164627874822290"}},"outputId":"3047e0e8-9de8-468d-b13e-5cfc365ec19e"},"execution_count":10,"outputs":[{"output_type":"stream","name":"stdout","text":["Trial 10 Complete [00h 00m 18s]\n","val_loss: 0.6225348114967346\n","\n","Best val_loss So Far: 0.5301704406738281\n","Total elapsed time: 00h 03m 09s\n"]},{"output_type":"stream","name":"stderr","text":["/usr/local/lib/python3.11/dist-packages/keras/src/layers/rnn/rnn.py:200: UserWarning: Do not pass an `input_shape`/`input_dim` argument to a layer. When using Seque